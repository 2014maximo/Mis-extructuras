<!DOCTYPE html>
<html lang="en">
<head>
    <data:blog.title/>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Trucos pa' desarrollo</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous"/>
    <b:skin>
    @import url('https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i');
    @import url('https://fonts.googleapis.com/css?family=Patua+One');
    @import url('https://fonts.googleapis.com/css?family=Open+Sans+Condensed:300,300i,700');

    .fuente {
        font-family: 'Open Sans', sans-serif;
    }
    .fuenteTitulo {
        font-family: 'Patua One', cursive;
    }
    .fuenteDos {
        font-family: 'Open Sans Condensed', sans-serif;
    }
    .tamanoUno {
        font-size: 27px;
    }
    .obscuro {
        background: #000;
    }
    .inputUno {
        border-top: 0;
        border-right: 0;
        border-left: 0;
        border-bottom: 1 white;
        height: 35px;
        width: 100%;
    }
    @media (max-width: 601px) {
    .grandes {
        display: none;
    }
    }
    @media (min-width: 600px) {
        .mobile {
            display: none;
        }
    }
    
    </b:skin>
</head>

<body>

<!-- HEADER -->
<div class="obscuro fixed-top text-primary pt-3 pb-3 grandes">
    <div class="row">
            <div class="col-1"></div>
            <div class="col-4">
                <h2 class="fuenteDos">TRUCOS PA' DESARROLLO</h2>
            </div>
            <div class="col-6">
                <form class="formUno">
                    <input class="inputUno text-light fuente bg-transparent" type="search" placeholder="Buscar" aria-label="Search"/>
                </form>
            </div>
            <div class="col-1"></div>
        </div>
    </div>
    <div class="pos-f-t mobile fixed-top">
        <div class="collapse" id="navbarToggleExternalContent">
          <div class="obscuro p-4">
            <form class="formUno">
                <input class="inputUno text-light fuente bg-transparent" type="search" placeholder="Buscar" aria-label="Search"/>
            </form>
          </div>
        </div>
        <nav class="navbar navbar-dark bg-dark text-primary">
            <h2 class="fuenteDos">TRUCOS PA' DESARROLLO</h2>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggleExternalContent" aria-controls="navbarToggleExternalContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
        </nav>
    </div>
    <!-- FIN HEADER -->


    <!-- CAROUSEL -->
    <div id="demo" class="carousel slide mt-5" data-ride="carousel">
        <!-- Indicators -->
        <ul class="carousel-indicators">
          <li data-target="#demo" data-slide-to="0" class="active"></li>
          <li data-target="#demo" data-slide-to="1"></li>
          <li data-target="#demo" data-slide-to="2"></li>
        </ul>
        
        <!-- The slideshow -->
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img src="https://4.bp.blogspot.com/-YFDH1JAYZXg/XpUZ6IvLVCI/AAAAAAAACSU/BiEz6N6jQp0NKv4QrHRBk_yPefGaVm35wCLcBGAsYHQ/s1600/slider-1.jpg" alt="Los Angeles" class="img-fluid"/>
          </div>
          <div class="carousel-item">
            <img src="https://2.bp.blogspot.com/-PYEfKDPfi-0/XpUZ6gqYEUI/AAAAAAAACSc/xg_LrrwNeC0EQ3x0OZc6fBRdDG8cHrK_QCLcBGAsYHQ/s1600/slider-2.jpg" alt="Chicago" class="img-fluid"/>
          </div>
          <div class="carousel-item">
            <img src="https://4.bp.blogspot.com/-Sfgh1rUUD10/XpUZ6De4pcI/AAAAAAAACSY/DpGZjNhl_UAkJaHKL0xJhn4-pQm2luz7QCLcBGAsYHQ/s1600/slider-4.jpg" alt="Chicago" class="img-fluid"/>
          </div>
        </div>
        
        <!-- Left and right controls -->
        <a class="carousel-control-prev" href="#demo" data-slide="prev">
          <span class="carousel-control-prev-icon"></span>
        </a>
        <a class="carousel-control-next" href="#demo" data-slide="next">
          <span class="carousel-control-next-icon"></span>
        </a>
    </div>
    <!-- FIN CAROUSEL -->


<!-- CONTENIDO POSTS -->
<b:section id="Header">
    <b:widget id="Header1" type="Header">
        <b:includable id="main">
        </b:includable>
    </b:widget>
</b:section>


<b:if cond='data:blog.url == data:blog.homepageUrl'>
<b:section class='mainblog' id='mainblog' showaddelement='no'>
  <b:widget id='Blog1' locked='true' title='Entradas del blog' type='Blog' version='1'>

  </b:widget>
</b:section>
</b:if>
<!-- FIN CONTENIDO POST -->

